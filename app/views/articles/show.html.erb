<div class="container">

	<h1>Article</h1>

	<div class="articles">
		<div class="article">
			<h1><%= @article.title %></h1>
			<p><%= @article.body %></p>
			<%= link_to 'edit', edit_article_path(@article) %>
			<%= link_to 'delete', article_path(@article), method: :delete, data: {confirm: "Really delete this?"} %>
		</div>
		
		<%= link_to "<<< Back To All Articles", articles_path, class: 'allArticle' %>
		
	</div>
</div>